func weight< int numHydrogen, int numOxygen > -> float {
    const g := 6.02E23 .
    const hy := 1.008 .
    const ox := 15.999 .
    const molecularWeight := hy * [numHydrogen |float] + ox * [numOxygen |float].
    return molecularWeight * g .
}
exec {
    weight(^a^, 2 + 3).
    call weight(^a^, ^b^).
}