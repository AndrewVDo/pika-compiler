func holler <int number> -> string {
	print number; isOdd(number);.
	return ["ab", "bc", "cd", "de", "ef", "fg", "gh"][number].
}
func isOdd <int number> -> bool {
	return number != (number / 2) * 2.
}


exec {
	const max := 5.
	
	var count := 0.
	while (count <= max) {
		var result := holler(count).
		print result,_n_.
		count := count + 1.
	}
	
	print max*max; isOdd(max*max), _n_.
	
	print "all done.", _n_.
}
