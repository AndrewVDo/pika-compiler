func holler <float number> -> null {
	var n := 0.
	print number, _n_.
	while (n < number) {
		print "Yoo hoo!!!!", _n_.
		n := n + 1.
	}
	return.
}

exec {
	const max := 6.
	call holler(5.21).
	
	var n := 0.23.
	while (n <= max) {
		call holler(n).
		n := n + 0.95.
	}
	
	print "all done: ", n, ".", _n_.
}
